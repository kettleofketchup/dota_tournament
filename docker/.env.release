NODE_ENV="release"
RELEASE=true
VERSION="0.5.6"
DEBUG=false
TEST=false
DOMAIN=dota.kettle.sh
FRONTEND_HOST=localhost
FRONTEND_PORT=3000
BACKEND_HOST=localhost
BACKEND_PORT=8000

# SSR API URL - used by frontend during server-side rendering
# Default: http://backend:8000/api (Docker Compose service name)
SSR_API_URL=http://backend:8000/api

CERTBOT_EMAIL=your@email.org
# Optional (Defaults)
DHPARAM_SIZE=2048
ELLIPTIC_CURVE=secp256r1
RENEWAL_INTERVAL=8d
RSA_KEY_SIZE=2048
STAGING=0
USE_ECDSA=1

# Advanced (Defaults)
CERTBOT_AUTHENTICATOR=webroot
CERTBOT_DNS_PROPAGATION_SECONDS=""
DEBUG=0
USE_LOCAL_CA=0
